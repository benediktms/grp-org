{
  "name": "hive-mind",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "test:up": "docker-compose -f docker-compose.test.yml up -d",
    "test:down": "docker-compose -f docker-compose.test.yml down",
    "docker:build:web": "docker build --build-arg NEXT_PUBLIC_API_URI=$NEXT_PUBLIC_API_URI -f apps/web/Dockerfile . -t benediktms/hive-mind-web",
    "docker:build:api": "docker build -f apps/api/Dockerfile . -t benediktms/hive-mind-api",
    "docker:start": "docker-compose up -d && sh ./scripts/run-migrations.sh",
    "test:db": "yarn test:up && yarn prisma:migrate:deploy && nx affected --target=test --parallel",
    "start:api": "nx run api:serve",
    "start:web": "nx run web:serve",
    "build": "nx affected --target=build --parallel",
    "build:api:prod": "nx run api:build:production --generatePackageJson",
    "build:web:prod": "nx run web:build:production --generatePackageJson",
    "test": "dotenv -e .env.test -- yarn test:db",
    "test:e2e": "nx e2e web-e2e --headed",
    "lint": "nx affected --target=lint --parallel",
    "format": "nx format --write",
    "prepare": "husky install",
    "prisma:migrate": "npx prisma migrate dev --name",
    "prisma:migrate:deploy": "yarn prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "gen": "graphql-codegen --config codegen.yml"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "private": true,
  "dependencies": {
