{
  "root": "libs/server/data",
  "sourceRoot": "libs/server/data/src",
  "projectType": "library",
  "targets": {
    "create-migration": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "ts-node --project tsconfig.lib.json ../../../node_modules/.bin/typeorm migration:create ./migrations --pretty -f .env.local",
        "cwd": "libs/server/data"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/server/data/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/server/data"],
      "options": {
        "jestConfig": "libs/server/data/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
